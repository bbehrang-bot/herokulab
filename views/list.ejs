<% layout('layout') -%>
  <section class="bands">
<%for (var i = 0; i < arr.length; i++) {
  %>
  <div class="bands_row">
  <div class="bands_remove">
  <form method="post" action="/bands/delete">
  <input name="id" type="hidden" value='<%=arr[i]._id%>'>
  <input type="submit" />
  </form>
  </div>
  <div class="bands_name">
  <h2><%= arr[i].username %></h2></div>
  </div>
<%}%>

</section>
